snippet \mathrm
alias   mathrm
	\mathrm{${1}}

snippet \sqrt
alias   sqrt
	\sqrt{${1}}${0}

snippet \renewcommand
alias   renewcommand
	\renewcommand{${1}}{${2}}

# amsmath.sty {{{1
snippet dfrac
alias   \dfrac
	\dfrac{${1}}{${2}}${0}

snippet text
alias   \text
	\text{${1}}

# beamer.cls {{{1
snippet block
alias   \begin{block} \block
	\begin{block}{${1:#:TITLE}}
		${0}
	\end{block}

snippet alertblock
alias   \begin{alertblock} \alertblock
	\begin{alertblock}{${1:#:TITLE}}
		${0}
	\end{alertblock}

snippet exampleblock
alias   \begin{exampleblock} \exampleblock
	\begin{exampleblock}{${1:#:TITLE}}
		${0}
	\end{exampleblock}

# bm.cls {{{1
snippet \bm
alias   bm
	\bm{${1}}

# graphicx.sty {{{1
snippet \includegraphics
alias   includegraphics
	\includegraphics[${1}]{${2}}

# mathtools.sty {{{1
snippet matrix*
alias   \begin{matrix*} \matrix*
	\begin{matrix*}[${1:c}]
		${2}
	\end{matrix*}

snippet pmatrix*
alias   \begin{pmatrix*} \pmatrix*
	\begin{pmatrix*}[${1:c}]
		${2}
	\end{pmatrix*}

snippet bmatrix*
alias   \begin{bmatrix*} \bmatrix*
	\begin{bmatrix*}[${1:c}]
		${2}
	\end{bmatrix*}

