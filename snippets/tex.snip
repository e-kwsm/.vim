# physics.sty {{{1
snippet \dd
alias   dd
	\dd{${1}}

snippet dv
alias   \dv
	\dv{${1}}{${2}}${0}

snippet pdv
alias   \pdv
	\pdv{${1}}{${2}}${0}

snippet \var
alias   var
	\var{${1}}

# siunitx.sty {{{1
snippet num
alias   \num
	\num{${1}}${0}

snippet si
alias   \si
	\si{${1:#:unit}}${0}

snippet SI
alias   \SI
	\SI{${1:#:num}}{${2:#:unit}}${0}

snippet numlist
alias   \numlist
	\numlist{${1}}${0}

snippet numrange
alias   \numrange
	\numrange{${1:#:begin}}{${2:#:end}}${0}

snippet SIlist
alias   \SIlist
	\SIlist{${1:#:num}}{${2:#:unit}}${0}

snippet SIrange
alias   \SIrange
	\SIrange{${1:#:begin}}{${2:#:end}}{${3:#:unit}}${0}

# subcaption.sty {{{1
snippet subcaptionbox
alias   \subcaptionbox
  \subcaptionbox{${1:#:heading}}{${2:#:contents}}

snippet thesubfigure
alias   \thesubfigure
	\renewcommand*{\thesubfigure}{${1}{subfigure}${2}}

