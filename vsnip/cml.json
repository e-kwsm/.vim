{
  "<cml:cml>": {
    "prefix": ["<cml:cml", "<cml"],
    "body": [
      "<?xml version=\"1.0\" encoding=\"UTF-8\"?>",
      "<cml xmlns=\"http://www.xml-cml.org/schema\" xmlns:convention=\"http://www.xml-cml.org/convention/\" convention=\"convention:molecular\">",
      "\t<molecule${0}",
      "</cml>"
    ]
  },
  "<cml:molecule>": {
    "prefix": ["<cml:molecule", "<molecule"],
    "body": [
      "<molecule id=\"${1:m0}\" formalCharge=\"${2:0}\">",
      "\t<atomArray${3}",
      "\t<bondArray${4}",
      "</molecule>"
    ]
  },
  "<cml:atomArray>": {
    "prefix": ["<cml:atomArray", "<atomArray"],
    "body": [
      "<atomArray>",
      "\t<atom${1}",
      "</atomArray>"
    ]
  },
  "<cml:atom>": {
    "prefix": ["<cml:atom", "<atom"],
    "body": "<atom id=\"${1:a0}\" elementType=\"${2:Du}\" x3=\"${3:0}\" y3=\"${4:0}\" z3=\"${5:0}\"/>"
  },
  "<cml:bondArray>": {
    "prefix": ["<cml:bondArray", "<bondArray"],
    "body": [
      "<bondArray>",
      "\t<bond${1}",
      "</bondArray>"
    ]
  },
  "<cml:bond>": {
    "prefix": ["<cml:bond", "<bond"],
    "body": "<bond id=\"${1:b0}\" atomRefs2=\"${2}\" order=\"${3|S,D,T|}\"/>"
  }
}
