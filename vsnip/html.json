{
  "<html>": {
    "prefix": "<html",
    "body": [
      "<!DOCTYPE html>",
      "<html>",
      "\t<head>${1}",
      "\t</head>",
      "\t<body>",
      "\t\t${0}",
      "\t</body>",
      "</html>"
    ]
  },

  "<a>": {
    "prefix": "<a",
    "body": "<a href=\"${1}\">${2}</a>"
  },
  "<abbr>": {
    "prefix": "<abbr",
    "body": "<abbr title=\"${1}\">${2}</abbr>"
  },
  "<b>": {
    "prefix": "<b",
    "body": "<b>${1}</b>"
  },
  "<blockquote>": {
    "prefix": "<blockquote",
    "body": "<blockquote>${1}</blockquote>"
  },
  "<code>": {
    "prefix": "<code",
    "body": "<code>${1}</code>"
  },
  "<del>": {
    "prefix": "<del",
    "body": "<del>${1}</del>"
  },
  "<details>": {
    "prefix": "<details",
    "body": ["<details>", "\t<summary>${1}</summary>", "\t${2}", "</details>"]
  },
  "<dfn>": {
    "prefix": "<dfn",
    "body": "<dfn>${1}</dfn>"
  },
  "<div>": {
    "prefix": "<div",
    "body": "<div>${1}</div>"
  },
  "<dl>": {
    "prefix": "<dl",
    "body": ["<dl>", "\t<dt${1}", "</dl>"]
  },
  "<dt>": {
    "prefix": "<dt",
    "body": ["<dt>${1}</dt>", "<dl>${2}</dl>"]
  },
  "<em>": {
    "prefix": "<em",
    "body": "<em>${1}</em>"
  },
  "<h1>": {
    "prefix": "<h1",
    "body": "<h1>${1}</h1>"
  },
  "<h2>": {
    "prefix": "<h2",
    "body": "<h2>${1}</h2>"
  },
  "<h3>": {
    "prefix": "<h3",
    "body": "<h3>${1}</h3>"
  },
  "<h4>": {
    "prefix": "<h4",
    "body": "<h4>${1}</h4>"
  },
  "<h5>": {
    "prefix": "<h5",
    "body": "<h5>${1}</h5>"
  },
  "<h6>": {
    "prefix": "<h6",
    "body": "<h6>${1}</h6>"
  },
  "<i>": {
    "prefix": "<i",
    "body": "<i>${1}</i>"
  },
  "<img>": {
    "prefix": "<img",
    "body": "<img alt=\"${1}\" src=\"${2}\"/>"
  },
  "<ins>": {
    "prefix": "<ins",
    "body": "<ins>${1}</ins>"
  },
  "<kbd>": {
    "prefix": "<kbd",
    "body": "<kbd>${1}</kbd>"
  },
  "<li>": {
    "prefix": "<li",
    "body": "<li>${1}</li>"
  },
  "<mark>": {
    "prefix": "<mark",
    "body": "<mark>${1}</mark>"
  },
  "<ol>": {
    "prefix": "<ol",
    "body": ["<ol>", "\t<li${1}", "</ol>"]
  },
  "<q>": {
    "prefix": "<q",
    "body": "<q>${1}</q>"
  },
  "<s>": {
    "prefix": "<s",
    "body": "<s>${1}</s>"
  },
  "<strong>": {
    "prefix": "<strong",
    "body": "<strong>${1}</strong>"
  },
  "<sub>": {
    "prefix": "<sub",
    "body": "<sub>${1}</sub>"
  },
  "<sup>": {
    "prefix": "<sup",
    "body": "<sup>${1}</sup>"
  },
  "<table>": {
    "prefix": "<table",
    "body": ["<table>", "\t<caption>${1}</caption>", "\t<tbody${2}", "</table>"]
  },
  "<tbody>": {
    "prefix": "<tbody",
    "body": ["<tbody>", "\t<tr${1}", "</tbody>"]
  },
  "<td>": {
    "prefix": "<td",
    "body": "<td>${1}</td>"
  },
  "<tfoot>": {
    "prefix": "<tfoot",
    "body": ["<tfoot>", "\t<tr${1}", "</tfoot>"]
  },
  "<th>": {
    "prefix": "<th",
    "body": "<th>${1}</th>"
  },
  "<thead>": {
    "prefix": "<thead",
    "body": ["<thead>", "\t<tr$1", "</thead>"]
  },
  "<time>": {
    "prefix": "<time",
    "body": "<time datetime=\"${1}\">${2}</time>"
  },
  "<tr>": {
    "prefix": "<tr",
    "body": ["<tr>", "\t<${1|td,th|}", "</tr>"]
  },
  "<ul>": {
    "prefix": "<ul",
    "body": ["<ul>", "\t<li${1}", "</ul>"]
  },
  "<var>": {
    "prefix": "<var",
    "body": "<var>${1}</var>"
  }
}
